{
  "name": "dev-profile",
  "version": "1.0.0",
  "description": "Professional Profile with Lit Element - Custom Agent Base Repository",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,html}\"",
    "test": "echo \"Tests will be added\" && exit 0"
  },
  "keywords": [
    "lit",
    "lit-element",
    "web-components",
    "frontend",
    "typescript"
  ],
  "author": "bfpard",
  "license": "MIT",
  "dependencies": {
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-lit": "^1.11.0",
    "eslint-plugin-lit-a11y": "^4.1.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
